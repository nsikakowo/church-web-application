f();\n\n  const getEventApi = async () =>{\n    const response = await axios.get(eventApi);\n    setEventTime(response.data.data.data);\n    console.log(response.data.data.data)\n  }\n\n  const startTimer = () => {\n    const countDownDate = new Date('September 11, 2021 04:15:00').getTime();\n\n    interval = setInterval(() => {\n      const now = new Date().getTime();\n      const distance = countDownDate - now;\n\n      const days = Math.floor(distance / (1000 * 60 * 60 * 24));\n      const hours = Math.floor((distance % (1000 * 60 * 60 * 24) / (1000 * 60 * 60)));\n      const minutes = Math.floor(distance % (1000 * 60 * 60 ) / (1000 * 60));\n      const seconds = Math.floor(distance % (1000 * 60 ) / 1000);\n\n      if (distance < 0 ){\n          //stop timer\n          clearInterval(interval.current);\n      }else{\n          setTimerDays(days);\n          setTimerHours(hours);\n          setTimerMinutes(minutes);\n          setTimerSeconds(seconds);\n      }\n    }, 1000);\n  }\n\n  \n  useEffect(() => {\n    startTimer();\n      clearInterval(interval.current);\n      getEventApi();\n  }, []);\n  \n  return (\n    <section className='timer-container'>\n      {eventTime.map((item, i)=> {\n        if(i === 0) {\n       return ( <div className='time-block' key ={item.id}>\n                        <div className=\"title\">\n                            <h3>Next Upcoming Event</h3>\n                            <p>{item.topic}</p>\n                        </div>\n                      \n                      <section className=\"timer\">\n                        <div>\n                          <p>{timerDays}</p>\n                          <p><small>Days</small></p>\n                        </div>\n                        \n                        <div>\n                          <p>{timerHours}</p>\n                          <p><small>Hours</small></p>\n                        </div>\n                          \n                        <div>\n                          <p> {timerMinutes} </p>\n                          <p><small>Minutes</small></p>\n                        </div>\n                          \n                        <div>\n                          <p> {timerSeconds} </p>\n                          <p><small>Seconds</small></p>\n                        </div> \n      </section>\n      </div>\n      ) }})}\n      <div className=\"service-reg\">\n        <NavLink to= './serviceregistration'>\n        <button>Service Registration</button>\n        </NavLink>\n      </div>\n       \n      </section>\n\n\n    \n  );\n}\n\nexport default NextEvent;\n","/Users/nsikakowo/Desktop/church-web-application/src/components/NeedPrayers/HomePrayerTab.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/App.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/screens/ServiceTimes.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/screens/Store.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/screens/NewLifeInChrist.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/screens/ContactUs.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/screens/Books.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/screens/ScrollTop.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/screens/KidsWhatshappening.js",["238"],"/Users/nsikakowo/Desktop/church-web-application/src/components/slider/Slider.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/components/connectGroups/ConnectGroup.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/components/beliefs/Beliefs.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/components/singleAboutResource/SingleResource.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/components/activities/Activities.js",[],"/Users/nsikakowo/Desktop/church-web-application/src/components/newMemberPlayer/MemberPlayer.js",["239"],"/Users/nsikakowo/Desktop/church-web-application/src/components/gallery/Gallery.js",[],"/Users/nsi